{
  "name": "travel",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "pwa": "pwa-asset-generator apps/ui/public/img/logo.svg ./apps/ui/public/pwa -i ./apps/ui/public/index.html -m ./apps/ui/public/site.webmanifest -b 'linear-gradient(to right, #122639 0%, #122639 100%)' --xhtml --favicon",
    "ui": "rimraf apps/ui/.next && nx run ui:serve --port 8080",
    "ui:build": "rimraf dist/apps/ui && nx run ui:build:production",
    "api": "nx run gateway:serve",
    "api:dev": "(cd apps/gateway/ && npx prisma migrate dev)",
    "api:show": "(cd apps/gateway/ && npx prisma studio)",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "prebuild": "rimraf dist",
    "prepare": "is-ci || husky install"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.2.1",
    "@nrwl/next": "^15.5.2",
    "@prisma/client": "^4.9.0",
    "@tanstack/react-query": "^4.24.4",
    "antd": "^5.1.5",
    "antd-img-crop": "^4.6.0",
    "argon2": "^0.30.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "core-js": "^3.27.1",
    "echarts": "^5.4.1",
    "echarts-for-react": "^3.0.2",
    "facepaint": "^1.2.1",
    "gsap": "^3.11.4",
    "is-mobile": "^3.1.1",
    "isomorphic-dompurify": "^0.26.0",
    "jodit": "^3.24.4",
    "ky-universal": "^0.11.0",
    "minio": "^0.2.9",
    "multiparty": "^4.2.3",
    "next": "13.1.2",
    "next-pwa": "^5.6.0",
    "next-share": "^0.19.0",
    "next-sitemap": "^3.1.52",
    "passport-jwt": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "stylis-plugin-rtl": "^2.1.1",
    "swiper": "^9.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.10.2",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "15.5.2",
    "@nrwl/cypress": "15.5.2",
    "@nrwl/eslint-plugin-nx": "15.5.2",
    "@nrwl/jest": "15.5.3",
    "@nrwl/linter": "15.5.2",
    "@nrwl/nest": "15.5.3",
    "@nrwl/node": "15.5.3",
    "@nrwl/react": "^15.5.2",
    "@nrwl/web": "^15.5.2",
    "@nrwl/webpack": "15.5.3",
    "@nrwl/workspace": "15.5.2",
    "@testing-library/react": "13.4.0",
    "@types/cookie": "^0.5.1",
    "@types/jest": "28.1.1",
    "@types/minio": "^7.0.15",
    "@types/multiparty": "^0.0.33",
    "@types/node": "18.7.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "28.1.1",
    "commitizen": "^4.3.0",
    "cypress": "^10.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "^13.1.0",
    "nx": "15.5.2",
    "prettier": "^2.6.2",
    "prisma": "^4.9.0",
    "pwa-asset-generator": "^6.2.1",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md,css,scss,yml}": "prettier --write **/*.{js,ts,jsx,tsx,json,md,css,scss,yml}"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
